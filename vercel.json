{
  "version": 2,
  "name": "maias-mexican-art-machine",
  "builds": [
    {
      "src": "web/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "web/api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/web/api/$1.py"
    }
  ],
  "env": {
    "RUNPOD_API_KEY": "@runpod-api-key",
    "RUNPOD_ENDPOINT_ID": "@runpod-endpoint-id"
  }
}
